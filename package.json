{
    "name": "webservice",
    "version": "1.0.0",
    "description": "Exemple d'API REST",
    "main": "server.js",
    "scripts": {
        "build": "tsc",
        "dev": "npx nodemon ./src/app.ts",
        "start": "tsc && npm run dev",
        "start:prod": "node ./build/src/app.js",
        "seed": "npx prisma db seed",
        "lint": "eslint ./**/*.ts",
        "format": "eslint ./**/*.ts --fix",
        "test": "tsc && jest",
        "test:coverage": "tsc && jest --coverage",
        "test:watch": "tsc && jest --watch",
        "prepare":"node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\"",
        "docker:dev": "npm install && npx prisma generate && npx prisma migrate deploy && npm run seed && npm run test && npm start",
        "docker:prod": "npm install --production && npm run start",
        "deploy:gcp": "npm install && npm run build && npm run start:prod"
    },
    "keywords": [
        "nodejs",
        "typescript",
        "restapi",
        "crud"
    ],
    "author": "Thomas Morgana",
    "license": "ISC",
    "devDependencies": {
        "@commitlint/cli": "^18.2.0",
        "@commitlint/config-conventional": "^18.1.0",
        "@faker-js/faker": "^8.0.2",
        "@types/bcryptjs": "^2.4.6",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/express-pino-logger": "^4.0.4",
        "@types/jest": "^29.5.8",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/node": "^20.4.5",
        "@types/pino": "^7.0.5",
        "@types/supertest": "^2.0.15",
        "@types/swagger-jsdoc": "^6.0.3",
        "@types/swagger-ui-express": "^4.1.6",
        "@typescript-eslint/eslint-plugin": "^6.9.0",
        "@typescript-eslint/parser": "^6.9.0",
        "eslint": "^8.52.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-jest": "^27.4.3",
        "eslint-plugin-prettier": "^5.0.1",
        "husky": "^8.0.0",
        "jest": "^29.7.0",
        "nodemon": "^3.0.1",
        "pino-pretty": "^10.2.3",
        "prettier": "^3.0.3",
        "prisma": "^5.5.2",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "validate-branch-name": "^1.3.0"
    },
    "dependencies": {
        "@prisma/client": "^5.5.2",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.20.2",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-pino-logger": "^7.0.0",
        "express-rate-limit": "^7.1.1",
        "http-status-codes": "^2.3.0",
        "jsonwebtoken": "^9.0.1",
        "pino": "^8.16.1",
        "prom-client": "^15.0.0",
        "redis": "^4.6.10",
        "stripe": "^14.3.0",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.0",
        "typescript": "^5.2.2"
    },
    "prisma": {
        "schema": "src/prisma/schema.prisma",
        "seed": "ts-node src/prisma/seeds"
    }
}
